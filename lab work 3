 x=5;
a=hamdan()
print(a.x)'''



class Person:
 def __init__(self,name,age):
  self.name=name
  self.age=age       
p1 = Person("hamdan",16)
print(p1.name)
print(p1.age)        


class hamdan:
 def __init__(self,name,age):
  self.name=name
  self.age=age
 def myfun(self):
  print("hello my name is:"+ self.name)
  print("hello my age is:", self.age)
     
p1=hamdan("ahmad",30)
p1.myfun()
            
        
class Person:
 def __init__(self,fname,lname):
  self.firstname=fname
  self.lastname=lname; 
 def printname(self):
  print(self.firstname,self.lastname)
  
class student(Person):
 def __init__(self,fname,lname):
  Person.__init__(self,fname,lname)
x=student("hamdan","raja")
x.printname()


class Person:
 def __init__(self, fname, lname):
  self.firstname = fname
  self.lastname = lname
 def printname(self):
  print(self.firstname, self.lastname)
class Student(Person):
 def __init__(self, fname, lname):
  super().__init__(fname, lname)
x = Student("Mike", "Olsen")
x.printname()
